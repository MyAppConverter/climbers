<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myappconverter.mobile"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-feature android:glEsVersion="0x00020000" />

    <application
        android:name="GenericCustomApp"
        android:allowBackup="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/icon"
        android:label="@string/climbers"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.myappconverter.mobile.Cocos2dxMainActivity"
            android:configChanges="orientation"
            android:label="@string/climbers"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.myappconverter.mobile.GameActivity"
            android:configChanges="orientation"
            android:label="@string/climbers"
            android:screenOrientation="portrait"
 
            android:theme="@android:style/Theme.NoTitleBar.Fullscreen" >
        </activity>
    </application>

</manifest>